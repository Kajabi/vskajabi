{
  "name": "kajabi-sage",
  "displayName": "Kajabi sage",
  "description": "Kajabi sage",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:kajabi-sage.openSageReact",
    "onCommand:kajabi-sage.openSageDocs",
    "onCommand:kajabi-sage.reactButtonPrimary",
    "onCommand:kajabi-sage.reactButtonAccent",
    "onCommand:kajabi-sage.reactButtonIcon",
    "onCommand:kajabi-sage.reactButtonLoading"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "kajabi-sage.openSageReact",
        "title": "Sage React Storybook"
      },
      {
        "command": "kajabi-sage.openSageDocs",
        "title": "Sage Docs"
      },
      {
        "command": "kajabi-sage.reactButtonPrimary",
        "title": "Sage Button Primary"
      },
      {
        "command": "kajabi-sage.reactButtonAccent",
        "title": "Sage Button Accent"
      },
      {
        "command": "kajabi-sage.reactButtonIcon",
        "title": "Sage Button Icon"
      },
      {
        "command": "kajabi-sage.reactButtonLoading",
        "title": "Sage Button Loading"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  }
}
